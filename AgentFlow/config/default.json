{
  "project": {
    "name": "my-project",
    "type": "fullstack",
    "description": "Default project configuration"
  },
  "workflow": {
    "maxParallel": 5,
    "reviewThreshold": 10,
    "mergeStrategy": "squash",
    "branchStrategy": "gitflow",
    "autoMerge": true,
    "requireTests": true,
    "minCoverage": 80
  },
  "claude": {
    "model": "claude-opus-4-1-20250805",
    "maxTokens": 4000,
    "temperature": 0.7,
    "useVision": true
  },
  "git": {
    "remote": "origin",
    "mainBranch": "main",
    "featurePrefix": "agent/",
    "commitFormat": "conventional",
    "signCommits": false
  },
  "testing": {
    "frameworks": ["jest", "cypress"],
    "runOnMerge": true,
    "failOnError": true,
    "coverageReports": true
  },
  "notifications": {
    "enabled": false,
    "channels": ["slack", "email"],
    "events": ["review_complete", "merge_complete", "error"]
  },
  "monitoring": {
    "dashboardPort": 3000,
    "logLevel": "info",
    "metricsEnabled": true,
    "tracingEnabled": false
  },
  "paths": {
    "logs": ".agentflow/logs",
    "worktrees": ".agentflow/worktrees",
    "reviews": ".agentflow/reviews",
    "artifacts": ".agentflow/artifacts",
    "cache": ".agentflow/cache"
  }
}
