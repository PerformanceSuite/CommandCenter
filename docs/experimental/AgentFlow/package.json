{
  "name": "agentflow",
  "version": "1.0.0",
  "description": "Autonomous Multi-Agent Claude Code Orchestration System",
  "main": "index.js",
  "scripts": {
    "start": "./scripts/agentflow.sh",
    "setup": "./scripts/setup.sh",
    "web": "open web/index.html",
    "dashboard": "node scripts/server.js",
    "monitor": "./scripts/monitor.sh --live",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testMatch='**/*.unit.test.js'",
    "test:integration": "jest --testMatch='**/*.int.test.js'",
    "lint": "eslint scripts/**/*.js",
    "format": "prettier --write '**/*.{js,json,md}'"
  },
  "keywords": [
    "claude",
    "ai",
    "automation",
    "agents",
    "orchestration",
    "development",
    "workflow"
  ],
  "author": "AgentFlow Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.11",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "node-fetch": "^3.3.2",
    "simple-git": "^3.20.0",
    "glob": "^10.3.10",
    "yaml": "^2.3.4",
    "async": "^3.2.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.1",
    "@types/node": "^20.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agentflow.git"
  }
}
